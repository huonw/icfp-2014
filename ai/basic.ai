(const UP 0)
(const RIGHT 1)
(const DOWN 2)
(const LEFT 3)

(const G_STANDARD 0)
(const G_FRIGHT 1)
(const G_INVISIBLE 2)

(const WALL 0)
(const EMPTY 1)
(const PILL 2)
(const POWER_PILL 3)
(const FRUIT 4)
(const LAMBDA_MAN_START 5)
(const GHOST_START 6)

(defun main ()
  (cons 0 step))

; walk in each of the three directions.
(defun step (state world)
  (if
      (eq state 3)
      (cons 0 state)
    (cons (add state 1) state)))
